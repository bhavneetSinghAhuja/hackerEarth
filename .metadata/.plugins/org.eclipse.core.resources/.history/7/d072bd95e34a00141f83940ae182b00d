#include <iostream>
#include <algorithm>

using namespace std;

void chefAndGround(){

	int t,n,m,a[100];
	int sum;
	cin>>t;

	for(int i=0; i<t;i++){
		sum=0;
		cin>>n;
		cin>>m;

		for(int j=0;j<n;j++){
			cin>>a[j];
		}

		sort(a, a+n);


		for(int j=0;j<(n-1);j++){
			sum+=(a[n-1]-a[j]);
		}

		if(sum==m)
			cout<<"Yes"<<endl;
		else
			cout<<"No"<<endl;

	}

}
int main(){chefAndGround();return 0;}

