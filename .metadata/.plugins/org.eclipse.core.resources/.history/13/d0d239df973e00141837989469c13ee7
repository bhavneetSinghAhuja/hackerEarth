#include <iostream>

using namespace std;

void rabbitUtil1(long int p, long int step){

	if((p-step)==0||(p-step-1)==0){
		cout<<step<<endl;
		return;
	}
	else{step++;
	cout<<p<<endl;
	cout<<step<<endl;
			rabbitUtil1(p-step-1,step);
			rabbitUtil1(p-step-2,step);
	}

}

void rabbitUtil(){

	long int p;
	cin>>p;

	rabbitUtil1(p,1);
}

void rabbit(){

	int t;
	cin>>t;
	for(int i=0;i<t;i++)
		rabbitUtil();
}

int main(){rabbit();return 0;}

