#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

void chefAndEasyProblem2(){
	int n, m ,q;
	cin>>n;
	cin>>m;
	cin>>q;
	int a[100][5000];
	//vector<vector<int> > a;

	for(int i=0;i<n;i++){
		for(int j=0;j<m;j++){
			cin>>a[i][j];
		}
	}

}

int main(){chefAndEasyProblem2(); return 0;}
