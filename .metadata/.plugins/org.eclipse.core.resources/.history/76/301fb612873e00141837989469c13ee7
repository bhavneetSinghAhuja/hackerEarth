#include <iostream>
#include <algorithm>

using namespace std;

void pinProblemUtil(){
	int n,m;
	cin>>n;
	cin>>m;
	int a[m];
	for (int i=0;i<m;i++)
		cin>>a[i];
	sort(a,a+m);
	int flag;
	int count=0;
	for(int i=1;i<=n;i++){
		flag=1;
		for(int j=(m-1);j>=0;j--){

			if(i%a[j]!=0){
				flag=0;
				break;
			}

		}
		if(flag==1)count++;
	}
	cout<<count<<endl;
	return;
}

void pinProblem(){

	int t;
	cin>>t;
	for(int i=0;i<t;i++)
		pinProblemUtil();
	return;
}
int main(){pinProblem();return 0;}
